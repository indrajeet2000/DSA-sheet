class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int k=1,n=nums[0];
        for(int i=1;i<nums.size();++i)
        {
            if(n!=nums[i])
            {
                nums[k]=nums[i];
                n=nums[i];
                k++;
            }
            else
            {
                
            }
        }
        return k;
    }
};
